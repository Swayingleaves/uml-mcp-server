{
  "mcpServers": {
    "uml": {
      "command": "python",
      "args": [
        "uml_mcp_server.py"
      ],
      "cwd": "/absolute/path/to/UML-MCP-Server",
      "env": {
        "PYTHONPATH": "/absolute/path/to/UML-MCP-Server",
        "DEFAULT_OUTPUT_DIR": "/absolute/path/to/custom/output/directory"
      }
    }
  }
} 